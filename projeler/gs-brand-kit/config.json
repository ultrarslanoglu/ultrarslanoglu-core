{
  "app": {
    "name": "GS Brand Kit",
    "version": "1.0.0",
    "port": 5003,
    "debug": true
  },
  "database": {
    "type": "sqlite",
    "path": "./data/gs_brand_kit.db"
  },
  "branding": {
    "logo_max_size_mb": 5,
    "supported_formats": ["png", "jpg", "svg", "webp"]
  },
  "logging": {
    "level": "info",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  }
}