{
  "app": {
    "name": "GS Analytics Dashboard",
    "version": "1.0.0",
    "port": 5001,
    "debug": true
  },
  "database": {
    "type": "sqlite",
    "path": "./data/gs_analytics.db"
  },
  "analytics": {
    "data_retention_days": 90,
    "aggregation_interval": "1h"
  },
  "logging": {
    "level": "info",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  }
}